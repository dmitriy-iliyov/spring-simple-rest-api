CREATE TABLE doctors (id SERIAL PRIMARY KEY ,name VARCHAR(30) NOT NULL,specification VARCHAR(20) NOT NULL,number VARCHAR(13) NOT NULL,email VARCHAR(20) NOT NULL);
CREATE TABLE users (id SERIAL PRIMARY KEY,name VARCHAR(30) NOT NULL,email VARCHAR(20) NOT NULL,number VARCHAR(14) NOT NULL, password VARCHAR(20) NOT NULL);
CREATE TABLE accounts (user_id INT PRIMARY KEY, balance DOUBLE NOT NULL, FOREIGN KEY (user_id) REFERENCES users (id));